    <rule name="ITPFakeCookie" value="true">
        <group type="or">
            <group type="and">
                <eq field="OSFamily">iOS</eq>
                <eq field="BrowserName">MobileSafari</eq>
                <gte field="OSVersion">11</gte>
                <lt field="OSVersion">12.2</lt>
            </group>
            <group type="and">
                <eq field="OSFamily">MacOS</eq>
                <eq field="BrowserName">Safari</eq>
                <gte field="OSVersion">10.13</gte>
            </group>
        </group>
    </rule>
    <rule name="ITP" value="true">
        <group type="or">
            <group type="or">
                <group type="and">
                    <eq field="OSFamily">iOS</eq>
                    <eq field="BrowserName">MobileSafari</eq>
                    <gte field="OSVersion">11</gte>
                </group>
                <group type="and">
                    <eq field="OSFamily">MacOS</eq>
                    <eq field="BrowserName">Safari</eq>
                    <gte field="OSVersion">10.13</gte>
                </group>
            </group>
            <group type="or">
                <group type="and">
                    <eq field="BrowserName">Firefox</eq>
                    <gte field="BrowserVersion">69</gte>
                </group>
                <group type="and">
                    <eq field="BrowserName">MobileFirefox</eq>
                    <gte field="BrowserVersion">69</gte>
                </group>
                <group type="and">
                    <eq field="BrowserName">MobileFirefox</eq>
                    <eq field="OSFamily">iOS</eq>
                    <gte field="BrowserVersion">19</gte>
                </group>
                <group type="and">
                    <eq field="BrowserName">Edge</eq>
                    <gte field="BrowserVersion">80</gte>
                </group>
            </group>
            <group type="and">
                <eq field="BrowserName">YandexBrowser</eq>
                <gte field="YaITPExpVersion">50</gte>
                <lte field="YaITPExpVersion">99</lte>
            </group>
            <group type="and">
                    <eq field="OSFamily">iOS</eq>
                    <gte field="OSVersion">14</gte>
            </group>
        </group>
    </rule>
