                <gte field="OSVersion">13</gte>
                <gte field="BrowserVersion">76</gte>
            </group>
            <group type="and">
                <eq field="BrowserName">ChromeMobile</eq>
                <eq field="OSFamily">Android</eq>
                <gte field="BrowserVersion">76</gte>
            </group>
            <group type="and">
                <eq field="BrowserName">Opera</eq>
                <gte field="BrowserVersion">63</gte>
            </group>
            <group type="and">
                <neq field="BrowserName">Chrome</neq>
                <neq field="BrowserName">ChromeMobile</neq>
                <neq field="BrowserName">YandexBrowser</neq>
                <neq field="BrowserName">Opera</neq>
                <eq field="BrowserBase">Chromium</eq>
                <gte field="BrowserBaseVersion">76</gte>
            </group>
        </group>
