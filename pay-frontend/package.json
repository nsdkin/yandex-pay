    },
    "license": "ISC",
    "author": "Simon Moiseenko <stepler@yandex-team.ru>",
    "workspaces": [
        "_config",
        "packages/*",
        "services/*"
    ],
    "scripts": {
        "deps": "npm i && cd server && npm i",
        "build": "npm run build --workspaces --if-present",
        "deploy:static": "npm run deploy:static --workspaces --if-present",
        "start": "hivemind",
        "husky": "run-s husky:init husky:prec-commit",
        "husky:init": "husky install .husky",
        "husky:prec-commit": "husky add .husky/pre-commit 'npm run pre-commit'",
        "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,json}\"",
        "eslint": "eslint \"./**/*.{js,jsx,ts,tsx,json}\" --fix",
        "stylelint": "stylelint  \"./**/*.{css,scss}\" --fix",
        "lint": "run-p --continue-on-error eslint stylelint",
        "pre-commit": "lint-staged",
        "browserslist-update-db": "browserslist --update-db"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint"
        ],
        "*.{scss,css}": [
            "stylelint"
        ]
    },
