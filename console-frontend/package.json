{
  "name": "public-pay-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:proxy dev:next",
    "dev:next": "npm run build:theme && APP_PORT=3010 NODE_ENV=development node ./server.js",
    "dev:proxy": "node .config/dev-server/index.js",
    "dev:no-proxy": "npm run build:theme && next dev",
    "build": "run-s build:theme build:next",
    "build:next": "next build",
    "build:theme": "themekit build",
    "start": "next start",
    "format": "NEXT_PUBLIC_RESTAPIFY_OFF=true prettier --write \"{src,mocked-api,public}/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "lint": "NEXT_PUBLIC_RESTAPIFY_OFF=true next lint --fix && bash -c tsc -p tsconfig.json --noemit",
    "stylelint": "stylelint \"**/*.scss\" --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@bem-react/classname": "1.5.12",
    "@bem-react/core": "3.0.8",
    "@bem-react/di": "3.1.1",
    "@svgr/webpack": "6.2.1",
    "@yandex/ui": "3.23.2",
    "axios": "0.26.1",
    "express": "4.17.3",
    "formik": "2.2.9",
    "lodash.chunk": "4.2.0",
    "lodash.debounce": "4.0.8",
    "lodash.range": "3.2.0",
    "next": "12.1.0",
    "next-compose-plugins": "2.2.1",
    "next-global-css": "1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-input-mask": "2.0.4",
    "react-redux": "7.2.6",
    "react-responsive": "9.0.0-beta.6",
    "react-use": "17.3.2",
    "redux": "4.1.2",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "reselect": "4.1.5",
    "restapify": "2.3.4",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/express": "4.17.13",
    "@types/lodash.chunk": "4.2.6",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.range": "3.2.7",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-input-mask": "1.2.1",
    "@types/react-redux": "7.1.24",
    "@types/redux-logger": "3.0.9",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "@yandex/themekit": "1.6.7",
    "dotenv": "16.0.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.2",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "got": "11.8.3",
    "http-proxy-middleware": "2.0.6",
    "husky": "7.0.4",
    "lint-staged": "12.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.0",
    "sass": "1.49.9",
    "serialize-javascript": "6.0.0",
    "stylelint": "14.8.3",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.2.0",
    "stylelint-semantic-groups": "1.1.3",
    "typescript": "4.6.2"
  }
}
